    "body": "I'm not sure where you had trouble, this is all you need to avoid the extra interface:\n\n``` diff\ndiff --git a/OpenRA.Game/GameRules/WeaponInfo.cs b/OpenRA.Game/GameRules/WeaponInfo.cs\nindex cb151ee..973ee5b 100644\n--- a/OpenRA.Game/GameRules/WeaponInfo.cs\n+++ b/OpenRA.Game/GameRules/WeaponInfo.cs\n@@ -31,8 +31,7 @@ public class ProjectileArgs\n        public Target GuidedTarget;\n    }\n\n-   public interface IEffectSynced :  IEffect, ISync { }\n-   public interface IProjectile : IEffectSynced { }\n+   public interface IProjectile : IEffect { }\n    public interface IProjectileInfo { IProjectile Create(ProjectileArgs args); }\n\n    public sealed class WeaponInfo\ndiff --git a/OpenRA.Game/World.cs b/OpenRA.Game/World.cs\nindex fadab7f..e43672c 100644\n--- a/OpenRA.Game/World.cs\n+++ b/OpenRA.Game/World.cs\n@@ -31,7 +31,7 @@ public sealed class World : IDisposable\n        internal readonly TraitDictionary TraitDict = new TraitDictionary();\n        readonly SortedDictionary<uint, Actor> actors = new SortedDictionary<uint, Actor>();\n        readonly List<IEffect> effects = new List<IEffect>();\n-       readonly List<IEffectSynced> syncedEffects = new List<IEffectSynced>();\n+       readonly List<IEffect> syncedEffects = new List<IEffect>();\n        readonly Queue<Action<World>> frameEndActions = new Queue<Action<World>>();\n\n        public int Timestep;\n@@ -274,10 +274,8 @@ public void Remove(Actor a)\n                t.RemovedFromWorld(a);\n        }\n\n-       public void Add(IEffect b) { effects.Add(b); }\n-       public void Add(IEffectSynced b) { effects.Add(b); syncedEffects.Add(b); }\n-       public void Remove(IEffect b) { effects.Remove(b); }\n-       public void Remove(IEffectSynced b) { effects.Remove(b); syncedEffects.Remove(b); }\n+       public void Add(IEffect b) { effects.Add(b); if (b is ISync) syncedEffects.Add(b); }\n+       public void Remove(IEffect b) { effects.Remove(b); if (b is ISync) syncedEffects.Remove(b); }\n        public void RemoveAll(Predicate<IEffect> predicate) { effects.RemoveAll(predicate); syncedEffects.RemoveAll(predicate); }\n\n        public void AddFrameEndTask(Action<World> a) { frameEndActions.Enqueue(a); }\n@@ -338,7 +336,7 @@ public void TickRender(WorldRenderer wr)\n\n        public IEnumerable<Actor> Actors { get { return actors.Values; } }\n        public IEnumerable<IEffect> Effects { get { return effects; } }\n-       public IEnumerable<IEffectSynced> SyncedEffects { get { return syncedEffects; } }\n+       public IEnumerable<IEffect> SyncedEffects { get { return syncedEffects; } }\n\n        public Actor GetActorById(uint actorId)\n        {\n```\n\nThis will also fix all your travis errors.\n"
  },
