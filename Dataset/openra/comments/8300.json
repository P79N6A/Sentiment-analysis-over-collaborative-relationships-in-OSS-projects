      "starred_url": "https://api.github.com/users/matija-hustic/starred{/owner}{/repo}",
      "subscriptions_url": "https://api.github.com/users/matija-hustic/subscriptions",
      "organizations_url": "https://api.github.com/users/matija-hustic/orgs",
      "repos_url": "https://api.github.com/users/matija-hustic/repos",
      "events_url": "https://api.github.com/users/matija-hustic/events{/privacy}",
