      "avatar_url": "https://avatars0.githubusercontent.com/u/1300387?v=4",
      "gravatar_id": "",
      "url": "https://api.github.com/users/nipunn1313",
      "html_url": "https://github.com/nipunn1313",
      "followers_url": "https://api.github.com/users/nipunn1313/followers",
