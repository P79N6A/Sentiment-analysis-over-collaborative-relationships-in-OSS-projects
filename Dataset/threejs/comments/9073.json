nderR = function( camera ){ THREE.glTFShaders.update( scene, camera ) };\n\n\n\/\/ VREffect.js\nvar preRenderL = null;\nvar preRenderR = null;\n\n\/\/ render (...) {\n\n  if ( preRenderL )\n    preRenderL( cameraL );\n\n  renderer.render( scene, cameraL, renderTarget, forceClear );\n\n  if ( preRenderR )\n    preRenderR( cameraR );\n\n  renderer.render( scene, cameraR, renderTarget, forceClear );\n\n}\n\n```\n","created_at":"2016-07-27T00:24:18.000Z","html_url":"https:\/\/github.com\/mrdoob\/three.js\/issues\/9073#issuecomment-235446289","id":235446289,"issue_url":"https:\/\/api.github.com\/repos\/mrdoob\/three.js\/issues\/9073","node_id":"MDEyOklzc3VlQ29tbWVudDIzNTQ0NjI4OQ==","updated_at":"2016-07-27T00:24:18.000Z","url":"https:\/\/api.github.com\/repos\/mrdoob\/three.js\/issues\/comments\/235446289","user":{"following_url":"https:\/\/api.github.com\/users\/mkeblx\/following{\/other_user}","events_url":"https:\/\/api.github.com\/users\/mkeblx\/events{\/privacy}","avatar_url":"https:\/\/avatars0.githubusercontent.com\/u\/293795?v=4","url":"https:\/\/api.github.com\/users\/mkeblx","gists_url":"https:\/\/api.github.com\/users\/mkeblx\/gists{\/gist_id}","html_url":"https:\/\/github.com\/mkeblx","subscriptions_url":"https:\/\/api.github.com\/users\/mkeblx\/subscriptions","node_id":"MDQ6VXNlcjI5Mzc5NQ==","repos_url":"https:\/\/api.github.com\/users\/mkeblx\/repos","received_events_url":"https:\/\/api.github.com\/users\/mkeblx\/received_events","gravatar_id":"","starred_url":"https:\/\/api.github.com\/users\/mkeblx\/starred{\/owner}{\/repo}","site_admin":false,"login":"mkeblx","type":"User","id":293795,"followers_url":"https:\/\/api.github.com\/users\/mkeblx\/followers","organizations_url":"https:\/\/api.github.com\/users\/mkeblx\/orgs"}},{"index":3,"author_association":"COLLABORATOR","body":"After #10851, this issue appears to be fixed.","created_at":"2017-02-26T22:37:39.000Z","html_url":"https:\/\/github.com\/mrdoob\/three.js\/issues\/9073#issuecomment-282594252","id":2