x, color){\r\n\r\n\t\tif ( color === undefined ) color = 0xffff00;\r\n\r\n\t\tlet indices = new Uint16Array([ 0, 1, 1, 2, 2, 3, 3, 0, 4, 5, 5, 6, 6, 7, 7, 4, 0, 4, 1, 5, 2, 6, 3, 7 ]);\r\n\t\tlet positions = new Float32Array( [\r\n\t\t\tbox.min.x, box.min.y, box.min.z,\r\n\t\t\tbox.max.x, box.min.y, box.min.z,\r\n\t\t\tbox.max.x, box.min.y, box.max.z,\r\n\t\t\tbox.min.x, box.min.y, box.max.z,\r\n\t\t\tbox.min.x, box.max.y, box.min.z,\r\n\t\t\tbox.max.x, box.max.y, box.min.z,\r\n\t\t\tbox.max.x, box.max.y, box.max.z,\r\n\t\t\tbox.min.x, box.max.y, box.max.z\r\n\t\t]);\r\n\r\n\t\tlet geometry = new THREE.BufferGeometry();\r\n\t\tgeometry.setIndex( new THREE.BufferAttribute( indices, 1 ) );\r\n\t\tgeometry.addAttribute( 'position', new THREE.BufferAttribute( positions, 3 ) );\r\n\r\n\t\tlet material = new THREE.LineBasicMaterial( { color: color } ) ;\r\n\t\t\r\n\t\tsuper(geometry, material);\r\n\t}\r\n\r\n}\r\n```","created_at":"2017-05-22T15:44:26.000Z","html_url":"https:\/\/github.com\/mrdoob\/three.js\/pull\/11207#issuecomment-303138863","id":303138863,"issue_url":"https:\/\/api.github.com\/repos\/mrdoob\/three.js\/issues\/11207","node_id":"MDEyOklzc3VlQ29tbWVudDMwMzEzODg2Mw==","updated_at":"2017-05-22T15:44:26.000Z","url":"https:\/\/api.github.com\/repos\/mrdoob\/three.js\/issues\/comments\/303138863","user":{"following_url":"https:\/\/api.github.com\/users\/m-schuetz\/following{\/other_user}","events_url":"https:\/\/api.github.com\/users\/m-schuetz\/events{\/privacy}","avatar_url":"https:\/\/avatars1.githubusercontent.com\/u\/6705073?v=4","url":"https:\/\/api.github.com\/users\/m-schuetz","gists_url":"https:\/\/api.github.com\/users\/m-schuetz\/gists{\/gist_id}","html_url":"https:\/\/github.com\/m-schuetz","subscriptions_url":"https:\/\/api.github.com\/users\/m-schuetz\/subscriptions","node_id":"MDQ6VXNlcjY3MDUwNzM=","repos_url":"https:\/\/api.github.com\/users\/m-schuetz\/repos","received_events_url":"https:\/\/api.github.com\/users\/m-schuetz\/received_events","